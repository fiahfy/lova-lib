{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "ecmaFeatures": {
    "modules": true,
    "jsx": true,
    "experimentalObjectRestSpread": true
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "_": false,
    "$": false
  },
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-uses-react": 1
  },
  "extends": "eslint:recommended"
}
