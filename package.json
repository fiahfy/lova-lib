{
  "name": "lova-tool",
  "version": "1.0.0",
  "description": "Tools for Lord of Vermilion Arena",
  "keywords": [
    "OpenShift",
    "Node.js",
    "application",
    "openshift"
  ],
  "author": {
    "name": "fiahfy",
    "email": "d.fiahfy@gmail.com",
    "url": "http://fiahfy.blogspot.com/"
  },
  "homepage": "http://fiahfy.blogspot.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/fiahfy/lova-tool"
  },
  "engines": {
    "node": ">= 0.6.0",
    "npm": ">= 1.0.0"
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js",
    "jquery-lazyload": "./node_modules/jquery-lazyload/jquery.lazyload.js",
    "angular": "./node_modules/angular/angular.js",
    "angular-route": "./node_modules/angular-route/angular-route.js",
    "angular-touch": "./node_modules/angular-touch/angular-touch.js",
    "angular-ui-bootstrap": "./node_modules/angular-ui-bootstrap/ui-bootstrap.js",
    "angular-ui-select2": "./node_modules/angular-ui-select2/src/select2.js",
    "angulartics": "./node_modules/angulartics/src/angulartics.js",
    "angulartics-google-analytics": "./node_modules/angulartics-google-analytics/lib/angulartics-google-analytics.js",
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js",
    "flat-ui": "./node_modules/flat-ui/dist/js/flat-ui.js",
    "google-analytics": "./vendor/google-analytics.js",
    "angular-draggable": "./vendor/ngDraggable.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": {
      "exports": "$"
    },
    "jquery-lazyload": {
      "depends": "jquery"
    },
    "angular": {
      "depends": "jquery",
      "exports": "angular"
    },
    "angular-touch": {
      "depends": "jquery"
    },
    "angular-ui-bootstrap": {
      "depends": "jquery"
    },
    "angular-ui-select2": {
      "depends": "jquery"
    },
    "bootstrap": {
      "depends": "jquery"
    },
    "flat-ui": {
      "depends": "jquery"
    }
  },
  "dependencies": {
    "angular": "^1.4.7",
    "angular-route": "^1.4.7",
    "angular-touch": "^1.4.7",
    "angular-ui-bootstrap": "^0.13.4",
    "angular-ui-select2": "0.0.5",
    "angulartics": "^0.20.1",
    "angulartics-google-analytics": "^0.1.1",
    "bootstrap": "^3.3.5",
    "browserify": "^11.2.0",
    "browserify-shim": "^3.8.10",
    "cheerio-httpcli": "^0.3.3",
    "co": "^4.6.0",
    "commander": "^2.8.1",
    "flat-ui": "git://github.com/designmodo/Flat-UI.git#2.2.2",
    "gulp": "^3.9.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tsd": "0.0.4",
    "gulp-typescript": "^2.9.0",
    "gulp-watch": "^4.3.5",
    "jquery": "^2.1.4",
    "jquery-lazyload": "^1.9.7",
    "koa": "^1.0.0",
    "koa-route": "^2.4.2",
    "koa-send": "^1.3.1",
    "koa-static": "^1.4.9",
    "mongoose": "^4.1.8",
    "request": "^2.63.0",
    "supervisor": "^0.8.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "devDependencies": {
    "lwip": "git://github.com/kant2002/lwip.git#5f6a38c"
  },
  "bundleDependencies": [],
  "private": true,
  "scripts": {
    "start": "PWD=`pwd`; forever start -w --minUptime 1000 --spinSleepTime 1000 -a -l ${PWD}/tmp/forever/forever.log server.js",
    "poststart": "mongod --fork --logpath /dev/null --dbpath tmp/mongodb/data",
    "prestop": "mongo admin --eval 'db.shutdownServer()' > /dev/null",
    "stop": "forever stop server.js",
    "tail": "forever logs -f server.js"
  },
  "main": "server.js"
}
