    "moment": "2.10.6",
    "mongoose": "4.1.8",
    "node-loader": "0.5.0",
    "nvd3": "1.8.1",
    "react": "0.14.3",
    "react-dnd": "2.0.2",
    "react-dnd-html5-backend": "2.0.0",
