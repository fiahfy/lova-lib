    "flat-ui": "designmodo/Flat-UI#2.2.2",
    "flux": "2.1.1",
    "history": "1.13.1",
    "hoist-non-react-statics": "1.0.3",
    "html-loader": "0.4.0",
    "imports-loader": "0.6.5",
