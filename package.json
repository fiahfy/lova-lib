{
  "name": "lova-lib",
  "version": "1.0.0",
  "description": "Site for Lord of Vermilion Arena",
  "keywords": [
    "OpenShift",
    "Node.js",
    "application",
    "openshift"
  ],
  "author": {
    "name": "fiahfy",
    "email": "d.fiahfy@gmail.com",
    "url": "http://fiahfy.blogspot.com/"
  },
  "homepage": "http://fiahfy.blogspot.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/fiahfy/lova-tool"
  },
  "private": true,
  "engines": {
    "node": ">=4.2.2"
  },
  "main": "server.js",
  "scripts": {
    "poststart": "mongod --fork --logpath /dev/null --dbpath tmp/mongodb/data",
    "prestop": "mongo admin --eval 'db.shutdownServer()' > /dev/null",
    "server": "nodemon -V -w server.js server.js",
    "watch": "npm run build -- -w",
    "watch-client": "npm run build-client -- -w",
    "watch-server": "npm run build-server -- -w",
    "watch-cli": "npm run build-cli -- -w",
    "build": "webpack --config webpack/all.babel.js",
    "build-client": "webpack --config webpack/client.babel.js",
    "build-server": "webpack --config webpack/server.babel.js",
    "build-cli": "webpack --config webpack/cli.babel.js",
    "lint": "eslint src",
    "dump": "mongodump --host=127.0.0.1 --port=27017 -u admin -d lova -o tmp/mongodb/dump",
    "restore": "mongorestore --host=127.0.0.1 --port=27017 -d lova --drop tmp/mongodb/dump/lova"
  },
  "dependencies": {
    "babel-core": "6.3.17",
    "babel-loader": "6.2.0",
