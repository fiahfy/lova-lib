  "bundleDependencies": [],
  "private": true,
  "scripts": {
    "start": "PWD=`pwd`; forever start -w --minUptime 1000 --spinSleepTime 1000 -a -l ${PWD}/tmp/forever/forever.log --watchDirectory ${PWD} server.js",
    "poststart": "mongod --fork --logpath /dev/null --dbpath tmp/mongodb/data",
    "prestop": "mongo admin --eval 'db.shutdownServer()' > /dev/null",
    "stop": "forever stop server.js",
    "tail": "forever logs -f server.js",
    "watch": "webpack -w",
    "dump": "mongodump --host=127.0.0.1 --port=27017 -u admin -d lova -o tmp/mongodb/dump",
    "restore": "mongorestore --host=127.0.0.1 --port=27017 -d lova --drop tmp/mongodb/dump/lova"
  },
  "main": "server.js"
}
